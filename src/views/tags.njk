---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
permalink: /tags/{{ tag | slug }}/
eleventyComputed:
  title: "{{ tag | title }}"
  description: "All posts tagged “{{ tag }}”"
---

{% extends "../layouts/base.njk" %}

{% block content %}

<h1>{{ tag | title }}</h1>

{% set postslist = collections[tag] %}
{% include "posts-list.njk" %}




{% endblock content %}
